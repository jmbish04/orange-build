{
  "$schema": "https://json.schemastore.org/wrangler",
  "name": "orange-build-test",
  "compatibility_date": "2024-12-12",
  "main": "worker/index.ts",
  
  // D1 Database binding
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "orange-build-db",
      "database_id": "4c1be3f5-98c4-40d9-8dc6-cdbbc88eb8ad"
    }
  ],

  // Durable Objects - define locally for tests
  "durable_objects": {
    "bindings": [
      {
        "name": "CodeGenObject",
        "class_name": "CodeGeneratorAgent"
      }
    ]
  },

  // AI binding (no need for token in tests)
  "ai": {
    "binding": "AI"
  },

  // Environment variables will be loaded from .dev.vars
  "vars": {}
}